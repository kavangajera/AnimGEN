{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "routes/animation.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "routes/animation.py"
      },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "PATH": "/usr/local/bin:/usr/bin:/bin:/opt/homebrew/bin",
      "FFMPEG_PATH": "/opt/homebrew/bin/ffmpeg",
      "LATEX_PATH": "/Library/TeX/texbin/latex"
    },
    "pythonVersion": "3.12"
  }
  